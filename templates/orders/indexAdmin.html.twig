{% extends 'baseAdmin.html.twig' %}

{% block body %}
    <div class="bs-example" data-example-id="striped-table">
        <table class="table table-striped">
            <thead>
            <tr>
                <th>ID</th>  
                <th>ID User</th>
                <th>Date</th>
                <th>Price Total</th>

            </tr>
            </thead>
            <tbody>
            {% for order in orders %}
                <tr>
                    <th scope="row">{{ order.id }}</th>
                    <td>{{ order.user.email }}</td>
                    <td>{{ order.dateOrder }}</td>
                    <td>{{ order.priceOrder }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}